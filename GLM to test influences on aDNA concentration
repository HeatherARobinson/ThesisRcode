data<-read.table("dnaconc.txt",header=TRUE)
attach(data)
head(data)
Year<-as.factor(Year)
modela<-lm(DNA~ Year*Mound+Sampletype)
summary(modela)
modelb<-update(modela,~. -Mound:Year)
anova(modela,modelb)
summary(modelb)
modelc<-update(modelb,~. -Year)  
anova(modelb,modelc)
summary(modelc)
modeld<-update(modelc,~. -Mound)
anova(modelc,modeld)
summary(modeld) 
